/**
 * Or gate:
 * out = (((a == 1) || (b == 1))), 1, 0) 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a,out=Nota);
    Not (in=b,out=Notb);
    Nand(a=Nota,b=Notb,out=out);
}
