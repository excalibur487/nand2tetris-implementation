/**
 * And gate:
 * out = (((a == 1) && (b == 1))), 1, 0) 
 */
CHIP And {
    IN a, b;
    OUT out;
    
    PARTS:
    Nand (a=a,b=b,out=aAndb);
    Not (in=aAndb,out=out);
}
