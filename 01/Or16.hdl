/**
 * 16-bit bitwise Or gate:
 * out[i] = (a[i] Or b[i]) for i = 0..15 
 */
CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    Not16 (in=a, out=nota);
    Not16 (in=b, out=notb);
    And16 (a=nota, b=notb, out=notaAndnotb);
    Not16 (in=notaAndnotb,out=out);
}
